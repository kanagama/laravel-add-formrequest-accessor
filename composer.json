{
    "name": "kanagama/laravel-add-formrequest-accessor",
    "description": "Laravel の FormRequest に accessor 機能を付与する",
    "keywords": [
        "laravel",
        "formrequest",
        "accessor"
    ],
    "authors": [
        {
            "name": "kazuma nagama",
            "email": "k.nagama0632@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^9.0 | ^10.0",
        "php": "^8.0 | ^8.1 | ^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 | ^10.0",
        "orchestra/testbench": "*",
        "phpstan/phpstan": "^1.10",
        "phpmetrics/phpmetrics": "^2.8",
        "php-coveralls/php-coveralls": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Kanagama\\FormRequestAccessor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanagama\\FormRequestAccessor\\Tests\\": "tests/"
        }
    },
    "exclude": [
        "tests/",
        ".docker/",
        ".github/",
        "*.md"
    ],
    "minimum-stability": "stable"
}
